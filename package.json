		"svelte": "^3.46.4",
		"svelte-icons": "^2.1.0",
		"svelte-loader": "^3.1.2",
		"tailwindcss": "^2.2.19",
		"ts-jest": "^27.1.3",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5",
		"vite": "^2.7.13",
		"xstate": "^4.30.1"
	},
	"type": "module",
	"engines": {
		"node": ">= 12.17.0"
	},
	"dependencies": {
