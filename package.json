		"@testing-library/svelte": "^3.0.3",
		"@tsconfig/svelte": "^2.0.1",
		"@types/jest": "^27.4.0",
		"@typescript-eslint/eslint-plugin": "^5.11.0",
		"@typescript-eslint/parser": "^5.11.0",
		"@xstate/svelte": "^0.2.1",
		"autoprefixer": "^10.4.2",
		"cssnano": "^5.0.17",
		"cypress": "^8.7.0",
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
